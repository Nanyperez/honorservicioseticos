.btn{display:inline-block;margin-top:1rem;padding:.75rem 1.5rem;background:var(--primary);color:var(--accent);text-decoration:none;border-radius:4px;font-weight:600}
.btn:hover{opacity:.9}
.card{background:var(--accent);padding:2rem;border-radius:8px;box-shadow:0 2px 8px rgba(0,0,0,.05);margin-bottom:2rem}
.cards{display:grid;grid-template-columns:repeat(auto-fit,minmax(280px,1fr));gap:1.5rem}
footer{background:#222;color:#ddd;text-align:center;padding:2rem}
